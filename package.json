{
  "name": "proofme",
  "version": "1.0.0",
  "description": "ProofMe Kiosk",
  "homepage": "https://www.proofme.id",
  "author": {
    "name": "ProofMe.iD",
    "email": "info@proofme.id"
  },
  "keywords": [
    "Kiosk",
    "ProofMe.Id",
    "Blockchain"
  ],
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "ELECTRON_GET_USE_PROXY=true electron-rebuild",
    "postinstall2": "DEBUG=electron-builder electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve --port=4208",
    "ng:serve:web": "ng serve -c web -o",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on tcp:4208 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:build": "npm run build:prod && electron-builder build",
    "electron:build:dir": "npm run build:prod && electron-builder build --linux dir",
    "electron:build:all": "npm run build:prod && electron-builder build -lmw",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "9.2.0",
    "@angular-devkit/build-angular": "0.1000.2",
    "@angular-eslint/builder": "0.0.1-alpha.32",
    "@angular-eslint/eslint-plugin": "0.0.1-alpha.32",
    "@angular-eslint/eslint-plugin-template": "0.0.1-alpha.32",
    "@angular-eslint/template-parser": "0.0.1-alpha.32",
    "@angular/cli": "10.0.2",
    "@angular/common": "10.0.3",
    "@angular/compiler": "10.0.3",
    "@angular/compiler-cli": "10.0.3",
    "@angular/core": "10.0.3",
    "@angular/forms": "10.0.3",
    "@angular/language-service": "10.0.3",
    "@angular/platform-browser": "10.0.3",
    "@angular/platform-browser-dynamic": "10.0.3",
    "@angular/router": "10.0.3",
    "@smilo-platform/smilo-commons-js-node": "^0.1.12",
    "@smilo-platform/smilo-commons-js-web": "^0.1.12",
    "@smilo-platform/web3": "1.0.0-beta.37",
    "@smilo-platform/web3-core-helpers": "1.0.0-beta.37",
    "@smilo-platform/web3-core-method": "1.0.0-beta.37",
    "@smilo-platform/web3-core-requestmanager": "1.0.0-beta.37",
    "@smilo-platform/web3-providers-http": "1.0.0-beta.37",
    "@smilo-platform/web3-providers-ws": "1.0.0-beta.37",
    "@smilo-platform/web3-utils": "1.0.0-beta.37",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@types/jasmine": "3.5.11",
    "@types/jasminewd2": "2.0.8",
    "@types/mocha": "7.0.2",
    "@types/node": "12.12.6",
    "@typescript-eslint/eslint-plugin": "3.6.0",
    "@typescript-eslint/eslint-plugin-tslint": "3.6.0",
    "@typescript-eslint/parser": "3.6.0",
    "chai": "4.2.0",
    "conventional-changelog-cli": "2.0.34",
    "core-js": "3.6.5",
    "cross-env": "7.0.2",
    "electron": "^9.2.1",
    "electron-builder": "22.7.0",
    "electron-reload": "1.5.0",
    "eslint": "7.4.0",
    "eslint-plugin-import": "2.22.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "5.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "6.3.0",
    "karma-jasmine": "3.3.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "mocha": "7.2.0",
    "npm-run-all": "4.1.5",
    "rxjs": "6.6.0",
    "ts-node": "8.10.2",
    "tslib": "2.0.0",
    "typescript": "3.9.6",
    "wait-on": "5.0.1",
    "qrcode": "^1.4.4",
    "webdriver-manager": "12.1.7",
    "wrtc": "^0.4.4",
    "zone.js": "0.10.3",
    "@angular/animations": "^10.0.8",
    "@angular/cdk": "^10.1.3",
